{
  "name": "copilotkit-microsoft-agent-framework-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"bun run dev:ui\" \"npm run dev:agent\" --names ui,agent --prefix-colors blue,green --kill-others",
    "dev:debug": "LOG_LEVEL=debug bun run dev",
    "dev:agent": "./scripts/run-agent.sh || scripts/run-agent.bat",
    "dev:ui": "next dev --turbopack --port 3008",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "install:agent": "./scripts/setup-agent.sh || scripts\\setup-agent.bat",
    "postinstall": "bun run install:agent"
  },
  "dependencies": {
    "@ag-ui/client": "^0.0.40",
    "@copilotkit/react-core": "1.10.6",
    "@copilotkit/react-ui": "1.10.6",
    "@copilotkit/runtime": "1.10.6",
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@popperjs/core": "^2.11.8",
    "@react-jvectormap/core": "^1.0.4",
    "@react-jvectormap/world": "^1.1.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/postcss": "^4.0.9",
    "apexcharts": "^4.3.0",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "flatpickr": "^4.6.13",
    "prismjs": "^1.30.0",
    "react-apexcharts": "^1.7.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dropzone": "^14.3.5",
    "simplebar-react": "^3.3.0",
    "swiper": "^11.2.0",
    "tailwind-merge": "^2.6.0",
    "next": "^16.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.4",
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "postcss": "^8",
    "typescript": "^5"
  }
}
